<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aliz.khoshkshoooooo">

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true"></supports-screens>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name=".Login" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".SignUp" android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Validate" android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Main" android:screenOrientation="portrait"></activity>

    </application>


</manifest>
